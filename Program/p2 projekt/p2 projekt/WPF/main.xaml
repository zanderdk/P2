<Window x:Class="p2_projekt.WPF.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:p2_projekt.WPF"
        Title="Lobop" VerticalAlignment="Top" HorizontalAlignment="Left" Left="0" Top="0">
    <Grid SizeChanged="Grid_SizeChanged">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TabControl x:Name="tabController" Grid.RowSpan="1" Grid.Row="1"/>
        <Button Content="Log Ud" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" Click="LogUdClick"/>
    </Grid>
</Window>
