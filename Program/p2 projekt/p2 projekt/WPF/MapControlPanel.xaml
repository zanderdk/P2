<Window x:Class="p2_projekt.WPF.MapControlPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controllers="clr-namespace:p2_projekt.WPF"
        Title="MapControlPanel" Height="300" Width="300">
    <StackPanel>
        <controllers:InfolineControl x:Name="boatSpaceId" Title="Plads Id" >
        </controllers:InfolineControl>
        
        <ComboBox Name="statusDropdown">
            <ComboBoxItem>Optaget gæstebåd</ComboBoxItem>
            <ComboBoxItem>Ledig</ComboBoxItem>
            <ComboBoxItem>Optaget medlemsbåd</ComboBoxItem>
        </ComboBox>
        
        <Button Click="Button_Click">Ændr status</Button>
    </StackPanel>
</Window>
